<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="126.0" prefWidth="304.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="proyecto.chat.controller.NicknameController">
   <children>
      <Label layoutX="62.0" layoutY="14.0" prefHeight="30.0" prefWidth="181.0" text="Put your nickname">
         <font>
            <Font size="20.0" />
         </font></Label>
      <TextField fx:id="tfNickname" alignment="CENTER" layoutX="38.0" layoutY="52.0" prefHeight="25.0" prefWidth="228.0" promptText="Nickname" />
      <Button fx:id="btnAccept" layoutX="62.0" layoutY="87.0" mnemonicParsing="false" onMouseClicked="#accept" text="Aceptar" />
      <Button fx:id="btnCancel" layoutX="191.0" layoutY="87.0" mnemonicParsing="false" onMouseClicked="#cancel" text="Cancelar" />
   </children>
</AnchorPane>
